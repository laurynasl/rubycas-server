<!DOCTYPE html>
<html>
    <head>
        <meta http-equiv="Content-Type" content="text/html; charset=UTF-8">
        <link rel="stylesheet" type="text/css" href="/css/login-openID.css" />
        <script type="text/javascript" src="/js/jquery-1.3.2.js"></script>
        <script type="text/javascript" src="/js/jquery-ui-1.7.2.custom.min.js"></script>
        <script type="text/javascript" src="/js/imagetick.js"></script>
        <script type="text/javascript" src="/js/jquery.cacheimage.js"></script>
        <script type="text/javascript" src="/js/images_preloader.js"></script>
        <script type="text/javascript" src="/js/events.js"></script>
        <title>Login page</title>
    </head>
    <body>
        <table class="dock">
            <tbody><tr>
                    <td class="left">
                        <div class="dock-left">
                            <div class="shortcut"><table><tbody><tr><td><a href="/"><img src="/images/exvo_components/dock/desktop.png"/><br/>Desktop</a></td></tr></tbody></table></div>

                        </div>
                    </td>
                    <td class="center">
                        <div class="dock-center ui-sortable" id="dock-dd" unselectable="on" style="-moz-user-select: none;">
                            <div class="shortcut"><table><tbody><tr><td><a href="http://store.exvo.com"><img src="/images/exvo_components/dock/store.png"/><br/>Store</a></td></tr></tbody></table></div>
                            <div class="shortcut"><table><tbody><tr><td><a href=""><img src="/images/exvo_components/dock/entrepreneurship.png"/><br/>Entrepreneurship</a></td></tr></tbody></table></div>
                            <div class="shortcut"><table><tbody><tr><td><a href=""><img src="/images/exvo_components/dock/team.png"/><br/>Teams</a></td></tr></tbody></table></div>
                            <br style="clear:both" />
                        </div>
                    </td>
                    <td class="right">
                        <div class="dock-right">Exvo</div>
                    </td>
                </tr>
            </tbody></table>
        <div class="login-wraper">
            <h1>
                Sign in with your Open ID
            </h1>
            <form action="" method="post">
                <fieldset>
                    <legend></legend>
                    <h2>Enter your Open ID URL</h2>
                    <div class="input-field">
                        <div class="input">
                            <span class="left">  </span>
                            <input id="url" name="url" class="search-default animated_txt" type="text" value="Url" title="Url" />
                            <span class="right"> </span>
                        </div>
                        <div>
                            <a class="button">
                                <table>
                                    <tr>
                                        <td class="button-left"></td>
                                        <td class="button-middle">Login</td>
                                        <td class="button-right"></td>
                                    </tr>
                                </table>
                            </a>
                        </div>
                        <br style="clear:both" />
                    </div>
                    <div class="form-additional-info">
                        <div class="rem-me">
                            <input id="rem-me" type="checkbox" class="checkbox" /><label for="rem-me">Remember me</label>
                        </div>
                        <br style="clear:both" />
                    </div>
                    <div class="form-bottom">
                        <div class="form-bottom-l">
                            <a class="button">
                                <table>
                                    <tr>
                                        <td class="button-left"></td>
                                        <td class="button-middle">Create account</td>
                                        <td class="button-right"></td>
                                    </tr>
                                </table>
                            </a>
                        </div>
                        <div class="form-bottom-r">
                            <a class="button">
                                <table>
                                    <tr>
                                        <td class="button-left" ></td>
                                        <td class="button-middle">Sign in with Open ID</td>
                                        <td class="button-right"></td>
                                    </tr>
                                </table>
                            </a>
                        </div>
                        <br style="clear:both" />
                    </div>
                    </div>
                </fieldset>
            </form>
        </div>
    </body>
</html>
